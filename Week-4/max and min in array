//wap to find max and min element in an array
#include <stdio.h>

int main() {
   int n;
   printf("enter size of array");
   scanf("%d",&n);
   int arr[n];
   
   printf("enter element");
   for (int i=0; i<n; i++)
   {
       
       scanf("%d",&arr[i]);
   }
   
   int max=arr[0], min=arr[0];
   for (int i=0; i<n; i++)
   {
       if (arr[i]>max)
       {
           max=arr[i];
       }
       if (arr[i]<min)
       {
           min=arr[i];
       }
   }
   printf("max element of array is:%d\n",max);
   printf("min element of array is:%d\n",min);

    return 0;
}

output:
enter size of array5
enter element245
612
78
2252
445
max element of array is:2252
min element of array is:78


