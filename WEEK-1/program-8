// wap to check whether the number is armstrong or not.
#include <stdio.h>
#include <math.h>

int main() {
    int n;
    printf("enter a number");
    scanf("%d",&n);
    int c=0;
    int sum=0;
    int temp=n;
    int k=n;
    while (temp>0)
    {
        temp=temp/10;
        c=c+1;
    }
    while (n>0)
    {
        int d= n%10;
        int sum= sum+pow(d,c);
        n=n/10;
    }
    if(k==sum)
    {
        printf("num is armstrong");
    }
    else
    {
        printf("not a armstrong");
    }
    return 0;
}


enter a number121
not a armstrong

