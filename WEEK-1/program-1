// wap a program to calculate the profit or loss.
#include <stdio.h>

int main() {
    float cp,sp;
    printf("entre the cp and sp");
    scanf("%f%f",&cp,&sp);
    if (cp>sp)
    {
        printf("loss");
    }
    else
    {
        printf("profit");
    }
    return 0;
}



