// wap to input the marks of student in 5 subject then calculate percentage and cgpa, based on which print the category of student with all details.
#include <stdio.h>

int main() {
    int sapid;
    int s1,s2,s3,s4,s5;
    printf("/n entre the sapid");
    scanf("sapid of student %d",sapid);
    printf("entre marks of five subjects");
    scanf("%d%d%d%d%d",&s1,&s2,&s3,&s4,&s5);
    float avg=(s1+s2+s3+s4+s5)/5;
    float cgpa= avg/10;
    if (cgpa>9)
    {
        printf("outstanding");
    }
    
    if (cgpa>7.5 && cgpa<8.99)
    {
        printf("excellent");
    }
    if (cgpa>6.0 && cgpa<7.99)
    {
        printf("good");
    }
   
    return 0;
}



/n entre the sapid590024216
entre marks of five subjects80 70 60 50
outstanding
