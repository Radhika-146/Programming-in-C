// WAP a program to check whether the year is leap or not.
#include <stdio.h>

int main() {
    int year;
    printf("enter the year...");
    scanf("%d",&year);
    if (year%100==0 && year%400==0 )
    {
        printf("leap year");
    }
    else
    {
        if (year%100==0 && year%4==0)
        {
            printf("leap year");
        }
        else
        {
            printf("not a leap year");
        }
    }

    return 0;
}

#output:
enter the year...1700
leap year

=== Code Execution Successful ===
