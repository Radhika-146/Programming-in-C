// wap a program to check smallest among four numbers.
#include <stdio.h>

int main() {
    int n1,n2,n3,n4;
    printf("entre four numbers");
    scanf("%d%d%d%d",&n1,&n2,&n3,&n4);
    if (n1<n2 && n1<n3 && n1<n4)
    {
        printf("n1 is smallest %d",n1);
    }
    if (n2<n1 && n2<n3 && n2<n4)
    {
        printf("n2 is smallest %d",n2);
    }
    if (n3<n1 && n3<n2 && n3<n4)
    {
        printf("n3 is smallest %d",n3);
    }
    if (n4<n1 && n4<n2 && n4<n3)
    {
        printf("n4 is smallest %d",n4);
    }
    return 0;
}

entre four numbers40 60 70 100
n1 is smallest 40
